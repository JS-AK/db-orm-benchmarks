generator client {
  provider = "prisma-client-js"
  // output   = "./generated/client"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  id          String     @id     @default(uuid())
  email       String     @unique
  first_name  String?
  last_name   String?
  is_deleted  Boolean
  password    String?
  salt        String?
  created_at  DateTime
  updated_at  DateTime?
}
